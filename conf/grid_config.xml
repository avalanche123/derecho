<?xml version="1.0"?>
<grid>
<!-- 

This configuration file defines all the nodes on the grid, and how they are to be displayed.

Each node set (<nodes>) must define a row and column position in one of two ways:
1) A 'row' and/or 'col' attribute
2) A regex group. If there are two groups, the first is the row, the second is the group.

A node may match multiple node sets, but only the last matching definition in the first matching subset will be used. 

-->
    <subset name="Janelia Compute Cluster">
        
        <nodes pattern="h(\d+)u(\d+)" slots="16"/>
        
        <nodes pattern="h09u(\d+)" slots="32" row="9"/>
        
        <nodes pattern="f14u01" slots="7" row="10" col="1"/>
        <nodes pattern="f14u05" slots="7" row="10" col="2"/>
        <nodes pattern="f14u09" slots="7" row="10" col="3"/>
        <nodes pattern="f14u13" slots="7" row="10" col="4"/>
        <nodes pattern="f14u17" slots="7" row="10" col="5"/>
        <nodes pattern="f14u21" slots="7" row="10" col="6"/>
                
        <nodes pattern="f15u11" slots="7" row="10" col="8"/>
        <nodes pattern="f15u15" slots="7" row="10" col="9"/>
        <nodes pattern="f15u19" slots="7" row="10" col="10"/>
        <nodes pattern="f15u23" slots="7" row="10" col="11"/>
        <nodes pattern="f15u27" slots="16" row="10" col="12"/>
        
        <nodes pattern="f16u01" slots="7" row="10" col="14"/>
        <nodes pattern="f16u05" slots="7" row="10" col="15"/>
        <nodes pattern="f16u09" slots="7" row="10" col="16"/>
        <nodes pattern="f16u13" slots="7" row="10" col="17"/>
        <nodes pattern="f16u17" slots="7" row="10" col="18"/>
        <nodes pattern="f16u21" slots="7" row="10" col="19"/>
        <nodes pattern="f16u25" slots="7" row="10" col="20"/>
        <nodes pattern="f16u29" slots="7" row="10" col="21"/>
        <nodes pattern="f16u33" slots="16" row="10" col="22"/>
        <nodes pattern="f16u35" slots="16" row="10" col="23"/>
        
    </subset>
    <subset name="Janelia Legacy Cluster">
    
        <nodes pattern="f0*(\d+)u0*(\d+)" slots="8"/>
        <nodes pattern="e0*(\d+)u0*(\d+)" slots="16"/>
        
    </subset>
</grid>

